<launch>
    <master auto="start"/>
 <!-- Run the map server --> 
     <!-- <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)"/> -->

    <node name="map_server" pkg="map_server" type="map_server" args="$(find map_server)/assets/mymap2.yaml"/>

 <!--- Run AMCL --> 
    <include file="$(find amcl)/examples/amcl.launch" />

    <node pkg="move_base" type="move_base" respawn="true" name="move_base" output="screen">
        <rosparam file="$(find silver_retriever_2dnav)/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find silver_retriever_2dnav)/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find silver_retriever_2dnav)/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find silver_retriever_2dnav)/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find silver_retriever_2dnav)/base_local_planner_params.yaml" command="load" />
        <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
		<param name="conservative_reset_dist" type="double" value="3.0" />
		<param name="controller_frequency" type="double" value="15.0" />
    </node>
</launch> 
