<launch>
   <node pkg="rplidar_ros" type="rplidarNode" name="rplidarNode" output="screen">
  	<param name="serial_port"         type="string" value="/dev/ttyUSB2"/>  
	<param name="serial_baudrate"     type="int"    value="115200"/>
  	<param name="frame_id"            type="string" value="laser"/>
  	<param name="inverted"            type="bool"   value="false"/>
  	<param name="angle_compensate"    type="bool"   value="true"/>
  </node>
 <node pkg="odometry_source" type="odometry_source" name="odometry_source" output="screen">
 </node>
 <node pkg="robot_setup_tf_tutorial" type="tf_broadcaster" name="tf_broadcaster" output="screen">
 </node>
 <node pkg="robot_setup_tf_tutorial" type="tf_listener" name="tf_listener" output="screen">
 </node>

</launch> 
